{
  "package": {
    "name": "WhatsQt-git",
    "repo": "WhatsQt",
    "subject": "mjdev",
    "desc": "WhatsQt automated builds",
    "website_url": "https://github.com/mjdev/WhatsQt",
    "issue_tracker_url": "https://github.com/mjdev/WhatsQt/issues",
    "vcs_url": "https://github.com/mjdev/WhatsQt.git",
    "github_use_tag_release_notes": true,
    "github_release_notes_file": "RELEASE.txt",
    "licenses": ["GPL-3.0"],
    "labels": [],
    "public_download_numbers": true,
    "public_stats": true
  },
  "version": {
    "name": "@PROJECT_VERSION@-@GIT_COMMIT_ID@",
    "desc": "Unstable builds",
    "vcs_tag": "@GIT_COMMIT_ID@",
    "gpgSign": false
  },
  "files": [
    { "includePattern": "build/WhatsQt.deb", "uploadPattern": "WhatsQt-@PROJECT_VERSION@-@GIT_COMMIT_ID@.deb" },
    { "includePattern": "build/WhatsQt.rpm", "uploadPattern": "WhatsQt-@PROJECT_VERSION@-@GIT_COMMIT_ID@.rpm" },
    { "includePattern": "build/WhatsQt.dmg", "uploadPattern": "WhatsQt-@PROJECT_VERSION@-@GIT_COMMIT_ID@.dmg" }
  ],
  "publish": true
}